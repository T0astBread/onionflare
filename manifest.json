{
	"manifest_version": 2,
	"name": "onionflare",
	"description": "Makes Firefox look like the Tor Browser to avoid Cloudflare CAPTCHAs",
	"version": "0.3.0",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"applications": {
		"gecko": {
			"id": "onionflare@t0ast.cc"
		}
	}
}
